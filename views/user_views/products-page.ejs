    <script>
        const searchInput = document.getElementById('searchInput')
        searchInput.addEventListener('input', function() {
        applyFilter();
         });


        function applyFilter() {
            event.preventDefault()
            const categoryName = document.querySelector('input[name="categoryName"]:checked')?.value || '';
            const sort = document.querySelector('input[name="sort"]:checked')?.value || '';
            
            const searchInput = document.getElementById('searchInput').value || '';

    

    
            fetch(`/search-and-filter?categoryName=${categoryName}&sort=${sort}&searchInput=${searchInput}`)
            .then(response => response.json())
            .then(data => {
                console.log('Received data from backend:', data);
                updateHtmlWithProducts(data.productdata);
            })
            .catch(error => {
                // Handle errors if any
                console.error(error);
            });
    }
    function updateHtmlWithProducts(products) {
    // Clear existing content
    const productContainer = document.getElementById('myTabContent');
    productContainer.innerHTML = "";

    // Update HTML with new products
    products.forEach(product => {
        const productHtml = `
            <div class="col-lg-4 col-md-4 col-12 col-sm-6 mb-4">
                <div class="product-cart-wrap">
                    <div class="product-img-action-wrap">
                        <div class="product-img product-img-zoom">
                            <a href="/product-details/${product._id}">
                                <img class="default-img" src="/${product.productImg[0]}" alt="">
                                <img class="hover-img" src="/${product.productImg[1]}" alt="">
                            </a>
                        </div>
                        <div class="product-action-1">
                            
                        </div>
                        <div class="product-badges product-badges-position product-badges-mrg">
                            <!-- Add badges if needed -->
                        </div>
                    </div>

                    <div class="product-content-wrap">
                        <div class="product-category">
                            <a href="/cart">${product.productCategory}</a>
                        </div>
                        <h2><a href="/product-details/${product._id}">${product.productName}</a></h2>
                        <div class="rating-result" title="90%">
                            <!-- Add rating if needed -->
                        </div>
                        <div class="product-price">
                            <span>
                                ₹${(product.discountStatus === 'Active' && typeof product.discountPercent === 'number') ?
                                    (product.sellingPrice - (product.sellingPrice * product.discountPercent ) / 100) :
                                    product.sellingPrice}
                            </span>

                            <span class="old-price">₹${(product.discountStatus === 'Active' && typeof product.discountPercent === 'number') ?
                                product.sellingPrice : product.regularPrice}</span>
                            

                            <span class="save-price font-md color3 ml-5 text-danger" style="font-size: small;">
                                ${(product.discountStatus === 'Active' && typeof product.discountPercent === 'number') ?
                                    ` - ${product.discountPercent}%` :
                                    ''}
                            </span>

                        </div>
                        <div class="product-action-1 show">
                            <a aria-label="Add To Cart" class="addToCartButton action-btn hover-up" data-product-id="${product._id}" href="cart"><i class="fi-rs-shopping-bag-add"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        `;

        // Append the product HTML to the container
        productContainer.innerHTML += productHtml;
    });
}


    

