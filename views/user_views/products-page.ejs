                                            <label class="form-check-label text-capitalize"  name="categoryName" for="<%= category._id %>" ><%= category.catgName %></label>
                                        </div>
                                    </li>
                                <% }) %>
                                <% }else{ %>
                                    <li><a href="">No Categories Found</a></li>
                                <% } %>
